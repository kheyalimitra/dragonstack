":50,"column":33}},{"name":"Generation","generated":{"line":118,"column":34},"source":"components/Generation.js","original":{"line":50,"column":34}},{"generated":{"line":118,"column":44},"source":"components/Generation.js","original":{"line":50,"column":33}}],"sources":{"components/Generation.js":"import React, { Component } from 'react';\nimport { connect } from 'react-redux';\nimport { fetchGeneration } from '../actions/generations';\nimport fetchStates from '../reducers/fetchStates';\n\nconst MIN_DELAY = 3000;\nclass Generation extends Component {\n    timer = null;\n    componentDidMount() {\n        this.fetchNextGeneration();\n    }\n    componentWillUnmount() {\n        // necessary clean up \n        clearTimeout(this.timer);\n    }\n    fetchNextGeneration = () => {\n        this.props.fetchGeneration();\n        let delay  = new Date(this.props.generation.expiration).getTime() - new Date().getTime();\n        this.timer = setTimeout(() => this.fetchNextGeneration(), delay);\n        if (delay < MIN_DELAY ) {\n            delay = MIN_DELAY;\n        };\n    }\n    render() {\n        const { generation } = this.props;\n        if(generation.status ===  fetchStates.fetching) {\n            return <div> ... </div>;\n        }\n        if(generation.status ===  fetchStates.error) {\n            return <div> { generation.message } </div>;\n        }\n        return (\n            <div>\n                <h3> Generation {generation.gererationId}. Expires on:  </h3>\n                <h4> {new Date(generation.expiration).toString()} </h4>\n             </div>\n        )\n    }\n}\nconst mapStateToProps = state => {\n    const generation  = state.generation;\n    return { generation };\n};\n\nconst componentConnector = connect(\n    mapStateToProps,\n    {fetchGeneration}\n    );\n\nexport default componentConnector(Generation);\n"},"lineCount":null}},"error":null,"hash":"e5d86220cfe3a318436c5f5526362edb","cacheData":{"env":{}}}